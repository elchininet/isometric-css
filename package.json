{
  "name": "isometric-css",
  "version": "1.0.0",
  "description": "A JavaScript library to build ismoetric projections using CSS classes",
  "keywords": [
    "isometric",
    "projection",
    "isometric-projection",
    "css",
    "isometric-css",
    "draw",
    "drawing",
    "engineering",
    "engineering-drawing",
    "coordinates",
    "graphics",
    "typescript"
  ],
  "license": "GPL-3.0",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "@types/**/*",
    "constants/**/*",
    "utilities/**/*",
    "browser.d.ts",
    "browser.js",
    "index.d.ts",
    "index.js",
    "browser.js.LICENSE.txt"
  ],
  "author": "ElChiniNet",
  "private": false,
  "sideEffects": false,
  "scripts": {
    "build": "webpack && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "copy": "cp -r ./dist/. ./",
    "prepare": "yarn build && yarn copy",
    "version": "git add .",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "isometric-css": "file:.yalc/isometric-css",
    "jss": "^10.5.0",
    "jss-plugin-camel-case": "^10.5.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "clean-webpack-plugin": "^3.0.0",
    "ts-loader": "^8.0.14",
    "tscpaths": "^0.0.9",
    "typescript": "^4.1.3",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0"
  }
}
